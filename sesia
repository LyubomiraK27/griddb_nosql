#include <iostream>

using namespace std;

bool isTriangle(int side1, int side2, int side3)
{
    bool exist = true;
    side3 = 2*side1;

    if(((side1 > side2) || (side1 < side2)) > side3)
    {
       (((side2 + side3) > side1) ? exist : !exist);

    }
    else if(((side1 < side2)) || (side1> side2) < side3)
    {
        (((side1 + side2) > side3) ? exist : !exist);
    }
    else if(((side1 < side3) || (side1 > side3)) < side1)
    {
     (((side1 + side3) > side2) ? exist : !exist);
    }

    return exist;
}

int evenPositions(int side1, int side2)
{
    int sumOfDigits = 0;



    for(int i = side1; i <= side2; i++)
    {
        for(int j = 0; i%10 != 0; j++)
        {
            if(j%2 == 0)
            {
                sumOfDigits *= i%10;
                i /= 10;
            }
            else
            {
                i /= 10;
            }
        }
    }

    return sumOfDigits;
}

int main()
{
    int side1 = 0;
    int side2 = 0;
    cin >> side1;
    cin >> side2;
    int side3 = 2*side1;
    if(isTriangle(side1, side2, side3) == true)
    {
        cout << evenPositions(side1, side2) << endl;
    }
    else
    {
        cout << "NO" << endl;
    }



    return 0;
}


//2
#include <iostream>

using namespace std;



int main()
{
    int matrix[20][20];
    int arr[20];
    int rows = 0;
    int col = 0;
    bool isEqual = false;
    cin >> rows >> col;
    if(rows != col)
    {
        cout << "The rows should be equal to the columns. Please try again." << endl;
        cin >> rows >> col;
    }
    else
    {
        cout << "Enter values of the matrix: " << endl;
        for(int i = 0; i < col; i++)
        {
            for(int j = 0; j < rows; j++ )
            {
                cin >> matrix[i][j];

            }
            cout << endl;
        }

        cout << endl;
        cout << "Enter values of the array: " << endl;
        for(int i = 0; i < rows; i++)
        {
            cin >> arr[i];
        }

        for(int i = 0; i < rows; i++ )
        {
            for(int j = 0; j < col; j++)
            {
                if(matrix[j][i] == arr[j])
                {
                    isEqual = true;
                    continue;
                }
            }
        }
    }

        if(isEqual)
        {
            cout << "The elements of the array match the requirements. ";
        }
        else
        {
            cout << "The elements of the array do not match the requirements. ";

        }

    return 0;





}


//3

#include <iostream>

using namespace std;

bool existence(char symbol, char* arr)
{
    if(arr[0] == symbol)
    {
        return true;
    }
    else if((arr[0] != '\0') && (arr[0] != symbol))
    {
        return existence(symbol, arr +1);
    }
    else if(arr[0] == '\0')
    {
        return false;
    }


}

int main()
{
    char arr[20];
    char element;
    cout << "Enter an array: " << endl;
    cin >> arr;
    cout << "Enter a character:" << endl;
    cin >> element;
    if(existence(element, arr))
    {
        cout << "The character exists in the array." << endl;
    }
    else
    {
        cout << "The character does not exist in the array." << endl;
    }
    return 0;
}
